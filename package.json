{
  "name": "themindfulpug",
  "version": "1.0.0",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "emotion": "^10.0.23",
    "js-cookie": "^2.2.1",
    "material-ui": "^0.20.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-modal": "^3.11.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-web-vector-icons": "^1.0.2",
    "serve": "^11.3.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start:dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "format": "prettier --write src/**/*.ts src/**/*.tsx src/**/*.js src/**/*.jsx",
    "test": "react-scripts test --watchAll=false --passWithNoTests=true",
    "test:dev": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && gcloud app deploy --project=themindfulpug"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format",
      "pre-push": "yarn test"
    }
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^4.2.3",
    "prettier": "^1.19.1"
  }
}
